{
    "name": "beartropy/saml2",
    "version": "0.2.8",
    "description": "Multi-IDP SAML2 SP integration for Laravel using onelogin/php-saml",
    "license": "MIT",
    "keywords": [
        "laravel",
        "saml",
        "saml2",
        "sso",
        "idp",
        "sp",
        "onelogin",
        "beartropy"
    ],
    "require": {
        "php": "^8.2",
        "onelogin/php-saml": "^4.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Beartropy\\Saml2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "authors": [
        {
            "name": "Beartropy"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Beartropy\\Saml2\\Saml2ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
